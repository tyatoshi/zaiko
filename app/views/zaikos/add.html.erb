<%= render 'zaikos/nav-item' %>

<%= form_with(model: @zaiko, local: true) do |f| %>
  <div class="form-group">
    <%= f.text_area :content, class: 'form-control', rows: 1 %>
  </div>
  <div class="form-group">
    <%= f.text_area :number, class: 'form-control', rows: 1 %>
  </div>
  <div class="form-group">
    <%= f.file_field :image_name, class: 'form-control'%>
  </div>
    <%= f.submit '新規作成', class: 'btn btn-primary btn-block' %>
<% end %>